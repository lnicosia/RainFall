N* N::N(int nb)
{
	*this = 0x8048848; // Constante
	*(this + 104) = nb;
	return this;
}

void N::setAnnotation(char *str)
{
	size_t len;

	len = strlen(str);
	memcpy(this + 4, str, len);
	return;
}

void main(int ac, char **av)
{
	N *N1;
	N *N2;

	if (ac < 2)
		_exit(1);
	N1 = new N; // N class = 108 bytes
	N1->N(5);
	N2 = new N; // N class = 108 bytes again
	N2->N(6);
	N1->setAnnotation(av[1]);
	(**N2)(N2, N1);
	return 0;
}
